{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "url": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "imageUrl": {
      "type": "string",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)",
      "required": true,
      "unique": true
    },
    "summary": {
      "type": "richtext",
      "required": true
    },
    "published": {
      "type": "datetime",
      "required": true
    },
    "news_site": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::news-site.news-site",
      "inversedBy": "articles",
      "required": true
    },
    "launches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::launch.launch",
      "inversedBy": "articles"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "articles"
    }
  }
}
